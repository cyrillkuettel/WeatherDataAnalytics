CREATE TABLE city (ZIPCODE INTEGER NOT NULL, LATITUDE FLOAT, LONGITUDE FLOAT, NAME VARCHAR(255), PRIMARY KEY (ZIPCODE))
CREATE TABLE weather (WEATHERID INTEGER NOT NULL, DESCRIPTION VARCHAR(255), SUMMARY VARCHAR(255), PRIMARY KEY (WEATHERID))
CREATE TABLE weatherData (WEATHERDATAID INTEGER NOT NULL, CURRENT_TEMP_CELCIUS FLOAT, HUMIDITY FLOAT, PRESSURE INTEGER, TIMESTAMP TIMESTAMP, WEATHERID INTEGER, WINDDIRECTION FLOAT, WINDSPEED FLOAT, ZIPCODE INTEGER, PRIMARY KEY (WEATHERDATAID))
