CREATE TABLE city (ZIPCODE INTEGER NOT NULL, LATITUDE FLOAT, LONGITUDE FLOAT, NAME VARCHAR(255), PRIMARY KEY (ZIPCODE))
CREATE TABLE weather (WEATHERID INTEGER NOT NULL, DESCRIPTION VARCHAR(255), SUMMARY VARCHAR(255), PRIMARY KEY (WEATHERID))
CREATE TABLE weatherdata (WEATHERDATAID INTEGER NOT NULL, CURRENT_TEMP_CELCIUS FLOAT, HUMIDITY FLOAT, PRESSURE INTEGER, TIMESTAMP TIMESTAMP, WINDDIRECTION FLOAT, WINDSPEED FLOAT, zipCode INTEGER, weatherID INTEGER, PRIMARY KEY (WEATHERDATAID))
ALTER TABLE weatherdata ADD CONSTRAINT FK_weatherdata_weatherID FOREIGN KEY (weatherID) REFERENCES weather (WEATHERID)
ALTER TABLE weatherdata ADD CONSTRAINT FK_weatherdata_zipCode FOREIGN KEY (zipCode) REFERENCES city (ZIPCODE)
