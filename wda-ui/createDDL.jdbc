CREATE TABLE city (ZIPCODE INTEGER NOT NULL, LATITUDE FLOAT, LONGITUDE FLOAT, NAME VARCHAR(255), PRIMARY KEY (ZIPCODE))
CREATE TABLE weather (WEATHERID INTEGER NOT NULL, DESCRIPTION VARCHAR(255), SUMMARY VARCHAR(255), PRIMARY KEY (WEATHERID))
CREATE TABLE weatherdata (WEATHERDATAID INTEGER NOT NULL, HUMIDITY FLOAT, PRESSURE INTEGER, TEMP FLOAT, TIMESTAMP TIMESTAMP, WINDDIRECTION FLOAT, WINDSPEED FLOAT, city INTEGER, weather INTEGER, PRIMARY KEY (WEATHERDATAID))
ALTER TABLE weatherdata ADD CONSTRAINT FK_weatherdata_city FOREIGN KEY (city) REFERENCES city (ZIPCODE)
ALTER TABLE weatherdata ADD CONSTRAINT FK_weatherdata_weather FOREIGN KEY (weather) REFERENCES weather (WEATHERID)
